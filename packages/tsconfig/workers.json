{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Cloudflare Workers",
  "extends": "./base.json",
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "lib": ["esnext"],
    "strict": true,
    "sourceMap": true,
    "types": [
      "@cloudflare/workers-types"
    ],
    "outDir": "./dist"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
